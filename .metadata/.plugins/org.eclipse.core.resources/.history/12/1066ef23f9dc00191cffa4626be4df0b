package popupHandling;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.chrome.ChromeOptions;

public class NotificationPopup {

	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver", "/home/admin1/eclipse-workspace/Selenium/driver/chromedriver");
		ChromeOptions options  = new ChromeOptions();
		WebDriver driver = new ChromeDriver(options);
		driver.manage().window().maximize();
		driver.get("https://www.facebook.com/");
		String email = PropertyFile.
		driver.findElement(By.xpath("//input[@placeholder='Email address or phone number']")).sendKeys("");
		driver.close();

	}

}
